    {#'bundles/westeurotravelhotels/js/vendor/jquery.mousewheel.js'#}
    {#'bundles/westeurotravelhotels/vendor/fancybox/jquery.fancybox-1.3.1.js'#}
    {#'bundles/westeurotravelhotels/js/vendor/q.js'#}
    {#'bundles/sonataadmin/jquery/jquery.form.js'#}
