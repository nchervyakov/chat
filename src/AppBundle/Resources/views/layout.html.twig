    {% javascripts

        filter='?uglifyjs2'
        output='js/compiled/*.js'
    %}
        <script type="text/javascript" src="{{ asset_url }}" defer="defer"></script>
    {% endjavascripts %}
{% endblock %}

{% block body %}
    {{ parent() }}

    <div id="container">
        <header id="header" class="header">
            <div class="header_wrap">
                {# % embed 'WestEuroTravelHotelsBundle:Default:header.html.twig' %}{% endembed %#}
            </div>
        </header>

        <div class="wrap">
            <div class="page">
                {% block breadcrumbs %}
                    <!--breadcumb here  -->
                    <div class="breadcrumbs">
                        <ul>
                            <li><a href="{{ path('homepage') }}">{% trans %}Home{% endtrans %} </a> / </li>
                            <li>{% trans %}My Account{% endtrans %}</li>
                        </ul>
                    </div>
                {% endblock %}

                {% block page_title %}
                    <h1 class="page_title">{% block page_title_text %}{% endblock %}</h1>
                {% endblock %}

                {% for type, messages in app.session.flashbag.all() %}
                    {% for key, message in messages %}
                        <div class="flash-{{ type }}">
                            {{ message|trans({}, 'FOSUserBundle') }}
                        </div>
                    {% endfor %}
                {% endfor %}

                <p>
                {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
                    Authorized
                {% else %}
                    Not authorized
                {% endif %}
                </p>

                {% block page %}{% endblock %}
            </div>
        </div>

        <div class="footer_container">
            <footer class="footer">
                {# % embed 'WestEuroTravelHotelsBundle:Default:footer.html.twig' %}{% endembed %#}
            </footer>
        </div>
    </div>
{% endblock %}

